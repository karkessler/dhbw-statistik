   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 3.2 Histogramm"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Reaktionszeiten visualisieren\n",
    "zeiten = np.array([150, 180, 220, 250, 390, 410, 520, 650])\n",
    "\n",
    "# Automatische Klassenbildung\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.hist(zeiten, bins=4, edgecolor='black', color='lightcoral')\n",
    "plt.xlabel('Reaktionszeit (ms)')\n",
    "plt.ylabel('Häufigkeit')\n",
    "plt.title('Verteilung der Reaktionszeiten (automatische Bins)')\n",
    "plt.grid(axis='y', alpha=0.3)\n",
    "plt.show()\n",
    "\n",
    "# Mit manuellen Bins\n",
    "bins = [0, 200, 400, 600, 800]\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.hist(zeiten, bins=bins, edgecolor='black', color='lightgreen')\n",
    "plt.xlabel('Reaktionszeit (ms)')\n",
    "plt.ylabel('Häufigkeit')\n",
    "plt.title('Verteilung der Reaktionszeiten (manuelle Bins)')\n",
    "plt.grid(axis='y', alpha=0.3)\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 3.3 Empirische Verteilungsfunktion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Beispiel mit Noten\n",
    "daten = np.array([2, 3, 1, 3, 4, 2, 3, 1, 2, 5])\n",
    "daten_sortiert = np.sort(daten)\n",
    "n = len(daten)\n",
    "\n",
    "# Empirische Verteilungsfunktion berechnen\n",
    "y = np.arange(1, n+1) / n\n",
    "\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.step(daten_sortiert, y, where='post', linewidth=2, color='darkblue')\n",
    "plt.xlabel('x (Note)')\n",
    "plt.ylabel('F̂(x)')\n",
    "plt.title('Empirische Verteilungsfunktion')\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.xlim(0, 6)\n",
    "plt.ylim(0, 1.05)\n",
    "\n",
    "# Markierungen hinzufügen\n",
    "plt.scatter(daten_sortiert, y, color='red', s=50, zorder=5)\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "### 3.4 Boxplot"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Vergleich zweier Systeme\n",
    "np.random.seed(42)  # Für Reproduzierbarkeit\n",
    "system_a = np.random.normal(200, 30, 100)\n",
    "system_b = np.random.normal(250, 50, 100)\n",
